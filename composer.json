{
    "name": "lulubin/yii2-widget-videojs",
    "description": "An Yii2 widget for saying hello to videojs",
    "keywords": ["yii2", "extension", "widget", "videojs"],
    "homepage": "https://github.com/lulubin/yii2-widget-videojs",
    "type": "yii2-extension",
    "license": "BSD 3-Clause",
    "authors": [
        {
            "name": "lulubin",
            "email": "452936616@qq.com",
            "homepage": "https://luluqi.cn/"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0",
        "bower-asset/video.js": "~5.1.0"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "lulubin\\videojs\\": ""
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        },
        "asset-ignore-files": {
            "bower-asset/video.js": [
                "*/**",
                "!dist/*"
            ]
        }
    }
}
